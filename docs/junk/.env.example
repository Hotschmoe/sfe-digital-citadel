# Domain Configuration
DOMAIN=thestrongs.com
CLOUDFLARE_EMAIL=admin@thestrongs.com
CLOUDFLARE_API_KEY=your_cloudflare_api_key

# Storage Paths (NAS Mounted Volumes)
MEDIA_PATH=/nas/media
BACKUP_PATH=/nas/backups
DOCUMENTS_PATH=/nas/documents
PHOTOS_PATH=/nas/photos
BOOKS_PATH=/nas/books
CACHE_PATH=/nas/cache

# Security
JWT_SECRET=your_very_long_secret_key
ADMIN_PASSWORD=your_secure_admin_password

# VPN Configuration
WIREGUARD_SERVER_IP=10.0.0.1
WIREGUARD_PEERS=10

# Matrix Configuration
MATRIX_SERVER_NAME=matrix.thestrongs.com
MATRIX_REPORT_STATS=no

# Nextcloud Configuration
NEXTCLOUD_ADMIN_USER=admin
NEXTCLOUD_ADMIN_PASSWORD=your_secure_password
POSTGRES_PASSWORD=your_secure_db_password
POSTGRES_USER=nextcloud
POSTGRES_DB=nextcloud

# Wiki Configuration
WIKI_DB_NAME=wikijs
WIKI_DB_USER=wikijs
WIKI_DB_PASSWORD=your_secure_wiki_password

# Moodle Configuration
MOODLE_DB_NAME=moodle
MOODLE_DB_USER=moodle
MOODLE_DB_PASSWORD=your_secure_moodle_password
MOODLE_ADMIN_USER=admin
MOODLE_ADMIN_PASSWORD=your_secure_password

# Jupyter Configuration
JUPYTER_ADMIN_USER=admin
JUPYTER_ADMIN_PASS=your_secure_password

# Gitea Configuration
GITEA_DB_NAME=gitea
GITEA_DB_USER=gitea
GITEA_DB_PASSWORD=your_secure_gitea_password

# GitHub Backup
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_USER=your_github_username

# Financial Management
FIREFLY_DB_NAME=firefly
FIREFLY_DB_USER=firefly
FIREFLY_DB_PASSWORD=your_secure_firefly_password
FIREFLY_APP_KEY=your_32_char_random_string

# Media Management VPN
VPN_PROVIDER=your_vpn_provider
VPN_USERNAME=your_vpn_username
VPN_PASSWORD=your_vpn_password

# Bitwarden Configuration
SMTP_HOST=smtp.gmail.com
SMTP_FROM=noreply@thestrongs.com
SMTP_PORT=587
SMTP_SSL=true
SMTP_USERNAME=your_smtp_username
SMTP_PASSWORD=your_smtp_password

# Media Server
PLEX_CLAIM=claim-xxxxxx
JELLYFIN_CACHE=/nas/cache/jellyfin

# Gaming
MINECRAFT_MEMORY=4G
MINECRAFT_JAVA_OPTS=-XX:+UseG1GC

# Monitoring
GRAFANA_ADMIN_PASSWORD=your_secure_password 